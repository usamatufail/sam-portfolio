{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": ["$ABSTRACT_KEY", "$GOOGLE_CLIENT_EMAIL", "$GOOGLE_SERVICE_PRIVATE_KEY", "$SHEET_ID", "$SPREADSHEET_ID"]
}
